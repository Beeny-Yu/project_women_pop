    "word_dic = {}\n",
    "\n",
    "for word in w_word_cleaned:\n",
    "    if word not in word_dic:\n",
    "        word_dic[word] = 1 # changed from \"0\" to \"1\"\n",
    "    else:\n",
    "        word_dic[word] += 1\n",
    "        \n",
    "w_word_dic = sorted(word_dic.items(), key=lambda x:x[1], reverse=True)\n",
    "w_word_dic"
